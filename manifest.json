{
    "manifest_version": 3,
    "name": "emoji",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_icon": {
            "120": "icon.png"
        },
        "default_title": "Click Me",
        "default_popup": "popup.html"
    },
    "icons": {
        "120": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["content-script.js"]
        }
      ]
}